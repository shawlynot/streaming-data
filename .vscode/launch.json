    "version": "0.2.0",
    "configurations": [
        {
            "name": "Ingest",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/app/src/main_ingest.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/app/src",
            "envFile": "${workspaceFolder}/app/.env"
        },
        {
            "name": "Vol",
            "type": "debugpy",
            "request": "launch",
            "module": "scalene",
            "args": [
                "run",
                "${workspaceFolder}/app/src/main_vol.py"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/app/src",
            "envFile": "${workspaceFolder}/app/.env"
        }
    ]
}